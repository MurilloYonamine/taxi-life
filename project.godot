"luana": "res://dialogic/characters/luana.dch",
"namorado": "res://dialogic/characters/namorado.dch",
"taxista": "res://dialogic/characters/taxista.dch"
}
directories/dtl_directory={
"luana-timeline": "res://dialogic/timelines/chapter_1/luana-timeline.dtl"
}
